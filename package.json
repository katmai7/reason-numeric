{
  "name": "react-numeric",
  "version": "0.0.0",
  "main": "dist/bundle.common.js",
  "module": "dist/bundle.esm.js",
  "unpkg": "dist/bundle.min.js",
  "keywords": [
    "rollup",
    "reasonml",
    "input",
    "number",
    "numeric",
    "format"
  ],
  "author": "Vitalii Hudyma <gkatmai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "bsb -clean-world",
    "bsb-watch": "bsb -make-world -w",
    "bsb-build": "bsb -make-world",
