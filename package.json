    "cz-customizable": "^5.2.0",
    "husky": "^0.15.0-beta.16",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-bucklescript": "^0.6.1",
